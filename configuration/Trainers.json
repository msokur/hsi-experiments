{
  "HNO": {
    "TYPE":  "Several Output",
    "RESTORE": false,
    "FILES_TO_COPY": [["*.py"], ["data_utils", ",*.py"], ["models", "*.py"], ["data_utils", "data_loaders", "*dyn.py"],
                 ["scrips", "start_cv.job"], ["trainers", "*.py"], ["utils", "*.py"]],
    "WITH_SAMPLE_WEIGHTS": false,
    "MODEL": "paper_model",
    "LEARNING_RATE": 1e-4,
    "CUSTOM_OBJECTS": ["F1_score"],
    "BATCH_SIZE": 500,
    "SPLIT_FACTOR": 0.8,
    "EPOCHS": 50,
    "SMALLER_DATASET": false,
    "MODEL_CHECKPOINT": {
      "monitor": "val_f1_score",
      "save_best_only": true,
      "mode": "max"
    },
    "EARLY_STOPPING": {
      "enable": true,
      "monitor": "val_f1_score",
      "mode": "max",
      "min_delta": 0,
      "patience": 5,
      "restore_best_weights": true
    }
  },
  "paper_model": {
    "DROPOUT": 0.1
  },
  "inception_model": {
    "WITH_BATCH_NORM": false,
    "INCEPTION_FACTOR": 8,
    "DROPOUT": 0.1
  }
}